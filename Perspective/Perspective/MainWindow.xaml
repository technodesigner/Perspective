<Window x:Uid="Window_1" x:Class="Perspective.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Perspective" Loaded="Window_Loaded" WindowState="Maximized">
    <DockPanel x:Uid="dockPanel1" Name="dockPanel1">
        <TabControl x:Uid="tabControl1" Name="tabControl1" Width="200" DockPanel.Dock="Left">
            <TabItem x:Uid="TabItem_1" Header="Content" >
                <TreeView x:Uid="treeView1" Name="treeView1">
                    <TreeViewItem x:Uid="TreeViewItem_1" Header="My Test" />
                </TreeView>
            </TabItem>
            <TabItem x:Uid="TabItem_2" />
        </TabControl>
        <Frame x:Uid="frame1" Name="frame1" />
    </DockPanel>
</Window>
